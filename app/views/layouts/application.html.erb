<!DOCTYPE html>
<html>
<head>
  <title>TestImg</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div>
    <%= link_to 'Home', root_path %>
  </div>
  <div id="messages">
    <% if flash[:notice].present? %>
      <div id="notice"><%= flash[:notice] %></div>
    <% end %>
    <% if flash[:alert].present? %>
      <div id="alert"><%= flash[:alert] %></div>
    <% end %>
  </div>
<%= yield %>

</body>
</html>
